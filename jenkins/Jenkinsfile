pipeline {
    agent any

    stages {
        stage ('Initialize Environment') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "MAVEN_HOME = ${MAVEN_HOME}"
                    ''' 
            }
        }

        stage('BuildCode') {
            steps {
                sh 'mvn clean package'
            }
        }

        stage('TestCode') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
